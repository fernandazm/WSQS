#include <iostream>
using namespace std;

int a = 0, b = 1, c, n;

int main(){
  cout << "How many elements of the Fibonacci serie you want to calculate?";
  cin >> n;
  for (int i = 0; i< n; i++){
    if(i==0){
      cout << a << ","<< b << ","<< endl;
    }
    else {
      c = a + b;
      a = b;
      b = c;
      cout << c << endl;
    }
  }
}
