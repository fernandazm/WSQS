#include <iostream>
using namespace std;

int main(){
  int x;
  int y;
  cin >> x;
  cin >> y;
     if(x >= y){
        if(y< x){
        int difference = x - y;
        cout << "The difference of these numbres is:" << difference << endl;
        }
      }
      cout << "Your first number is:" << x << endl;
      cout << "Your second number is:" << y << endl;
