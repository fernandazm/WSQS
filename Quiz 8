#include <iostream>
#include <vector>
using namespace std;

int sumsquares_list(vector <int> square){
int sum = 0;
for(int i=0; i<square.size(); i++){
  sum = sum + (square[i] * square[i]);
}
return sum;
}

int main(){
  vector <int> square (5);
  square [0] = 1;
  square [1] = 2;
  square [2] = 3;
  square [3] = 4;
  square [4] = 5;
      cout << sumsquares_list(square)<< endl;
return 0;
}
